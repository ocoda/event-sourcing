{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"noImplicitAny": false,
		"removeComments": false,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es6",
		"sourceMap": false,
		"skipLibCheck": true,
		"strictNullChecks": true
	},
	"exclude": ["dist", "node_modules"]
}
